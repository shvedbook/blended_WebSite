<div style="margin: 5% auto;">
  <h3 style="text-align: center; font-size: 6vh; line-height: 0.4;">
    משאבי למידה ותוצרים
  </h3>
</div>
<div class="row">
  <div class="col-2"></div>
  <div class="col-8 row" style="text-align: center; font-size: 2.5vh; color: black;">
    <div class="col-md-3" style="border-left: 3px solid #646464;">
      <a style="color: black;" href="/products#facetoface">מודל תיוך מלא</a>
    </div>
    <div class="col-md-3" style="border-left: 3px solid #646464;">
      <a style="color: black;" href="/products#kvuzatImun">קבוצת אימון</a>
    </div>
    <div class="col-md-3" style="border-left: 3px solid #646464;">
      <a style="color: black;" href="/products#onlineblend">onlineBlend</a>
    </div>
    <div class="col-md-3">
      <a style="color: black;" href="/products#kitaAfuha">כיתה הפוכה</a>
    </div>
  </div>
  <div class="col-2"></div>
</div>
<div class="background1" style="padding-top: 10%;">
  <% var myModel; modelsData.forEach(model => { %>
  <div class="row models" id="<%=model.id%>">
    <% let colors = ["green","blue","yellow"]; let randColor = colors[Math.floor(Math.random() * 3)]
    %>
    <div class="col-12">
      <h2 class="col-md-3"><%=model.name%></h2>

      <div class="row">
        <% model.case_studies_products.forEach(product => { %>
        <div class="col-sm-3" style="padding: 1%; height: 200px;">
          <a href=" <%=product[0]%>" target="_blank">
            <img src="/images/madrasaLinkPic.png" style="width: 100%; max-height: 150px;" />
          </a>
          <h5 style="text-align: center; font-size: 4vh;"><%=product[1]%></h5>
        </div>
        <% }); %>
      </div>
    </div>
    <% }); %>
  </div>
</div>
<!-- FOOTER -->
<%-include(`./partials/footer`) %> <%-include(`./partials/topFunction`) %>
